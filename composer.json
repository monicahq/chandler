{
  "name": "monicahq/monica",
  "type": "project",
  "description": "Monica is a personal relationship manager.",
  "keywords": [
    "framework",
    "laravel",
    "crm",
    "prm"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "fruitcake/laravel-cors": "^2.2",
    "guzzlehttp/guzzle": "^7.4.5",
    "http-interop/http-factory-guzzle": "^1.2",
    "inertiajs/inertia-laravel": "^0.6.3",
    "itsgoingd/clockwork": "^5.1.6",
    "laravel-notification-channels/telegram": "^2.1",
    "laravel/framework": "^9.22.1",
    "laravel/sanctum": "^2.15.1",
    "laravel/scout": "^9.4.10",
    "laravel/tinker": "^2.7.2",
    "meilisearch/meilisearch-php": "^0.23.3",
    "tightenco/ziggy": "^1.4.6",
    "uploadcare/uploadcare-php": "^3.2.4",
    "vluzrmos/language-detector": "^2.3.3"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.12.3",
    "brianium/paratest": "^6.6.1",
    "fakerphp/faker": "^1.20",
    "laravel/breeze": "^1.11.2",
    "laravel/pint": "^0.1.7",
    "laravel/sail": "^1.15.1",
    "mockery/mockery": "^1.5",
    "nunomaduro/collision": "^6.2.1",
    "nunomaduro/larastan": "^2.1.12",
    "phpunit/phpunit": "^9.5.21",
    "spatie/laravel-ignition": "^1.3.1",
    "vimeo/psalm": "^4.25"
  },
  "autoload": {
    "psr-4": {
      "App\\": ["app/", "domains/"],
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi",
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
